g++ -m64 -O -fomit-frame-pointer -fPIC -g
g++ -m64 -fomit-frame-pointer -fPIC -g
g++ -m32 -O3 -fomit-frame-pointer -funroll-loops -fPIC -g
g++ -m32 -O -fomit-frame-pointer -fPIC -g
g++ -m32 -fomit-frame-pointer -fPIC -g
